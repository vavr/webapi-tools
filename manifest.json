{
  "manifest_version": 2,

  "name": "WebAPI Tools",
  "description": "В помощь разработчикам 2GIS",
  "version": "1.4.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["build/main.js"],
      "css": ["static/tip.css"],
      "all_frames": true
    }
  ],

  "permissions": [
    "activeTab",
    "<all_urls>"
  ],

  "web_accessible_resources": [
    "static/*"
  ]
}